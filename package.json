{
  "name": "@fanwang/pack-util",
  "type": "module",
  "version": "3.2.1",
  "description": "Pack all installed package in ./node_modules, and publish to Nexus self-hosted Registry.",
  "author": "fanwang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vampirefan/pack-util.git"
  },
  "keywords": [
    "npm",
    "pack",
    "publish"
  ],
  "bin": {
    "pack-util": "./bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 16.17.0"
  },
  "scripts": {
    "test": "node bin/index.js pack",
    "commit": "git add . && czg && git push",
    "release": "bumpp package.json bin/index.js"
  },
  "dependencies": {
    "commander": "9.5.0",
    "execa": "9.4.0",
    "form-data": "4.0.0",
    "inquirer": "9.3.7",
    "node-fetch": "3.3.2",
    "p-limit": "6.1.0",
    "yaml": "2.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.7.3",
    "bumpp": "9.6.1",
    "czg": "1.10.1",
    "taze": "0.17.2"
  }
}
